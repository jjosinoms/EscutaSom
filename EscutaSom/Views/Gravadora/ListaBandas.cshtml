@model IEnumerable<EscutaSom.Models.Banda>

@{
    ViewBag.Title = "ListaBandas";
}

<h2>ListaBandas </h2>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Nome)

            </td>
        </tr>
    }

</table>
