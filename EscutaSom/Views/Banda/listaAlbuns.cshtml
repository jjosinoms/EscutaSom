@model IEnumerable<EscutaSom.Models.Album>

@{
    ViewBag.Title = "listaAlbuns";
}

<h2>Albuns</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.ActionLink(item.Nome, "listaFaixas", new { id = item.Id }) 
            </td>
        </tr>
    }

</table>